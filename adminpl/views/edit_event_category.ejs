										<!--<div class="col-sm-2">
											<p class="help-block">Your help text!</p>
										</div>-->
									</div>				
									<div class="form-group">
										<label for="txtarea1" class="col-sm-2 control-label">Event Category Description: </label>
										<div class="col-sm-8">
											<textarea rows="5" cols="60" class="form-control" id="eventcategorydescription" name="category_description"><%= db_rows_array[0].category_description %></textarea> 
										</div>
									</div>		
									<div class="form-group">
										<label for="focusedinput" class="col-sm-2 control-label">Event Category Image: </label>
										<div class="col-sm-8">
											<input type="file" class="form-control" id="eventcategoryimage" name="category_image" placeholder="Enter Event Category photo" value="<%= db_rows_array[0].category_image %>">  
										</div>
										<!--<div class="col-sm-2">
											<p class="help-block">Your help text!</p>
										</div>-->
									</div>							
									
									<div class="form-group">
										&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;      
